{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "@nuxtjs",
    "@nuxtjs/eslint-config-typescript"
  ],
  "rules": {
    "quotes": ["warn", "single"],
    "comma-dangle": ["warn", "always-multiline"],
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": "off",
    "vue/no-unused-vars": "off",
    "require-await": "warn",
    "no-unreachable": "warn",
    "vue/no-v-model-argument": "off",
    "vue/no-v-for-template-key": "off"
  }
}
